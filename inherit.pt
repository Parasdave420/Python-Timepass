# Simple implementation of inhgeritance in python.

class Mammal:
    def walk(self):
        print("walk")
        
        
class Dog(Mammal):        #inheriting Mammal class
    def barks(self):
        print("Barking")
    
    
class Cat(Mammal):
    pass
    
    
dog1 = Dog()
dog1.walk()
dog1.barks()

cat1 = Cat()
cat1.walk()
cat1.barks()      #It will throw error
